  <world name="cras_dev">
    <xacro:include filename="$(find vmrc_gazebo)/worlds/sandisland.xacro" />
    <xacro:sandisland />
    <include>
      <name>totem_0</name>
      <uri>model://dock_2018U</uri>
      <pose>10 -2.5 0 0 0 0</pose>
    </include>

    <include>
      <name>totem_1</name>
      <uri>model://dock_2018U</uri>
      <pose>-10 2.5 0 0 0 -3.1415</pose>
    </include>

  </world>
</sdf>
